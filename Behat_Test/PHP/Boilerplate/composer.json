{
    "type": "project",
    "license": "proprietary",
    "require": {
        "behat/behat": "^3.13",
        "symfony/console": "^6.4",
        "vlucas/valitron": "^1.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
